{
  "compilerOptions": {
    "experimentalDecorators": true,
    "jsx": "preserve",
    "target": "ES6",
    "module": "ESNext",
    "baseUrl": ".",
    "paths": {
      "*": ["./src/types/*"],
      "Src/*": ["./src/*"],
    },
    "sourceMap": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
  }
}